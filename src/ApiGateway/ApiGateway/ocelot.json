{
  "Routes": [
    {
      "DownstreamPathTemplate": "/login",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "authservice",
          "Port": 8081
        }
      ],
      "UpstreamPathTemplate": "/api/auth/login",
      "UpstreamHttpMethod": [ "POST" ]
    },
    {
      "DownstreamPathTemplate": "/register",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "authservice",
          "Port": 8081
        }
      ],
      "UpstreamPathTemplate": "/api/auth/register",
      "UpstreamHttpMethod": [ "POST" ]
    },
    {
      "DownstreamPathTemplate": "/users/{userId}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "authservice",
          "Port": 8081
        }
      ],
      "UpstreamPathTemplate": "/api/auth/users/{userId}",
      "UpstreamHttpMethod": [ "GET" ],
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "Bearer"
      }
    },
    {
      "DownstreamPathTemplate": "/api/BusLines/routes",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "routeservice",
          "Port": 8082
        }
      ],
      "UpstreamPathTemplate": "/api/routes/buslines/routes",
      "UpstreamHttpMethod": [ "GET" ]
    },
    {
      "DownstreamPathTemplate": "/api/BusReservation/reserve",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "reservationservice",
          "Port": 8083
        }
      ],
      "UpstreamPathTemplate": "/api/reservations/reserve",
      "UpstreamHttpMethod": [ "POST" ],
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "Bearer"
      }
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:5000"
  }
}